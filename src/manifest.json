{
  "manifest_version": 3,
  "name": "Linesnapper",
  "description": "Phishing detection extension with onchain ruleset",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["icon-128.png", "icon-34.png"],
      "matches": []
    }
  ],
  "permissions": ["activeTab", "storage"]
}
